{
  "name": "sia-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "dev:css": "tailwindcss -i ./assets/tailwind.css -o ./public/assets/app.css --watch",
    "build:css": "tailwindcss -i ./assets/tailwind.css -o ./public/assets/app.css --minify",
    "start": "node server.js",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15"
  }
}
